  ATOM C6   TYPE=CC6  CHARge= 0.050   END
  ATOM H61  TYPE=HAS  CHARge= 0.100   END
  ATOM H62  TYPE=HAS  CHARge= 0.100   END
  ATOM O6   TYPE=OH1  CHARge=-0.650   END
  ATOM HO6  TYPE=H    CHARge= 0.400   END

  BOND C1 O1  
  BOND C1 H1
  BOND O1 HO1
